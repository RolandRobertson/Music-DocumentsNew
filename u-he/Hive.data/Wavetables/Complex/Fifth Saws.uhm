// by Howard Scarr

Info "Two flavours of 5ths - sawtooth and organ. Try mixtures."

NumFrames = 2

// Saws
Wave start=0 end=0 "2*phase + 4*phase%2 + 6*phase%2 -3"

// Organ
Wave start=1 end=1 "2*phase + frac(3*phase) + frac(6*phase) + frac(12*phase)"
Wave start=1 end=1 "x + frac(16*phase) + frac(32*phase) + frac(40*phase) -4"

Spectrum lowest=0 highest=0 "0"
